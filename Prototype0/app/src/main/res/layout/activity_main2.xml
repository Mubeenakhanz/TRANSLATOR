<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Main2Activity">


    <Button
        android:id="@+id/goBackBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="296dp"
        android:background="@android:drawable/ic_media_rew"
        android:text="@string/secondPageBtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"

    <Button
        android:id="@+id/healthBtn"
        android:layout_width="88dp"
        android:layout_height="119dp"
        android:background="@drawable/healthpng"
        app:Layout_constraintBottom_toBottom="@id/foodBtn"
        app:layout_constraintHorizontal_chainStyle="spread"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="116dp" />

    <Button
        android:id="@+id/restroomBtn"
        android:layout_width="89dp"
        android:layout_height="95dp"
        android:background="@drawable/restroompng"
        app:Layout_constraintBottom_toBottom="@id/button3"
        tools:layout_editor_absoluteX="18dp"
        tools:layout_editor_absoluteY="525dp" />

    <Button
        android:id="@+id/busBtn"
        android:layout_width="91dp"
        android:layout_height="106dp"
        android:background="@drawable/ic_action_bus"
        app:Layout_constraintBottom_toBottom="@id/restroomBtn"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="393dp" />

    <Button
        android:id="@+id/foodBtn"
        android:layout_width="88dp"
        android:layout_height="115dp"
        android:background="@drawable/ic_action_restaurant"
        app:Layout_constraintBottom_toBottom="@id/busBtn"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="262dp" />

    <View
        android:id="@+id/divider"
        android:layout_width="409dp"
        android:layout_height="1dp"
        android:layout_marginTop="96dp"
        android:background="#9C27B0"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="more"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="646dp" />
</androidx.constraintlayout.widget.ConstraintLayout>